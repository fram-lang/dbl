data rec List X = [] | (::) of X, List X

let rec map {A,B,E} (f : A ->[E] B) (xs : List A) :[E] List B =
  match xs with
  | [] => []
  | x :: xs => f x :: map f xs
  end

let rec append {A} (xs : List A) : total List A ->[] List A =
  fn ys =>
  match xs with
  | [] => ys
  | x :: xs => x :: append xs ys
  end
